<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fast_food">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".detallesSandwich"
            android:parentActivityName=".segundo_sandwiches"
            android:label="@string/titulo_sandiw">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".segundo_sandwiches"/>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/titulo_app">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".segundo_sandwiches"
            android:parentActivityName=".MainActivity"
            android:label="@string/titulo_sandiw">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".tercero_nosotros"
            android:parentActivityName=".MainActivity"
            android:label="@string/titulo_app">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>

    </application>

</manifest>